@model IEnumerable<Zelik.Models.Movie>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <thead>
        <tr>
            <th>Movie</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var movie in Model)
        {
            <tr>
                <td>@Html.ActionLink(movie.Name,"Details", "Movies", new {id=movie.Id }, null)</td>
            </tr>
        }
    </tbody>
</table>

